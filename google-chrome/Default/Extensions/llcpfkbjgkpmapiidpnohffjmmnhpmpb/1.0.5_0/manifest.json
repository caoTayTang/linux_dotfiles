{
   "background": {
      "persistent": true,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_title": "BetterViewer"
   },
   "content_security_policy": "default-src 'self'; connect-src 'self' https://*/*",
   "icons": {
      "128": "icons/128.png",
      "16": "icons/16.png",
      "48": "icons/48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "BetterViewer",
   "permissions": [ "webRequest", "webRequestBlocking", "activeTab", "storage", "*://*/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.5",
   "web_accessible_resources": [ "pages/*" ]
}
